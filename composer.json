{
    "name": "yiisoft/yii-cycle",
    "type": "library",
    "description": "Cycle ORM support",
    "keywords": [
        "yii",
        "cycle",
        "orm",
        "migrations"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-cycle/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii-cycle"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "cycle/annotated": "^2.0",
        "cycle/migrations": "^1.0",
        "cycle/orm": "^1.2",
        "psr/event-dispatcher": "^1.0",
        "psr/simple-cache": "^1.0",
        "yiisoft/aliases": "^1.0",
        "yiisoft/data": "@dev",
        "yiisoft/friendly-exception": "^1.0",
        "yiisoft/injector": "^1.0.2",
        "yiisoft/yii-console": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "roave/infection-static-analysis-plugin": "^1.3",
        "vimeo/psalm": "^4.1",
        "yiisoft/test-support": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Yii\\Cycle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Yii\\Cycle\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "common": "config/common.php",
            "console": "config/console.php",
            "events-console": "config/events-console.php"
        }
    },
    "scripts": {
        "test": [
            "phpunit --colors=always",
            "infection --no-progress",
            "psalm"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
