{
    "name": "yiisoft/yii-cycle",
    "type": "library",
    "description": "Cycle ORM support",
    "keywords": [
        "yii",
        "cycle",
        "orm",
        "migrations"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-cycle/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii-cycle"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "cycle/annotated": "^2.0",
        "cycle/migrations": "^1.0",
        "cycle/orm": "^1.1",
        "yiisoft/composer-config-plugin": "^1.0@dev",
        "psr/event-dispatcher": "^1.0",
        "psr/simple-cache": "^1.0",
        "yiisoft/aliases": "^1.0",
        "yiisoft/data": "@dev",
        "yiisoft/friendly-exception": "@dev",
        "yiisoft/yii-console": "@dev"
    },
    "require-dev": {
        "infection/infection": "^0.16.3",
        "phan/phan": "^3.0.2",
        "phpunit/phpunit": "^9.2",
        "yiisoft/di": "@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Yii\\Cycle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Yii\\Cycle\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "common": "config/common.php",
            "console": "config/console.php"
        },
        "config-plugin-dev": {
            "tests": [
                "$common",
                "$console",
                "config/tests.php"
            ]
        }
    },
    "scripts": {
        "test": [
            "phpunit --colors=always",
            "infection --no-progress",
            "phan --no-progress-bar"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
